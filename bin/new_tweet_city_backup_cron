#!/bin/bash
RBENV_ROOT=/usr/local/rbenv
PATH=/usr/local/rbenv/shims:/usr/local/rbenv/bin:/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin:/usr/games

cd /home/ubuntu/workspace/new_tweet_city
. .env

AWS_S3_SECRET=$AWS_S3_SECRET AWS_S3_KEY=$AWS_S3_KEY \
 bundle exec backup perform -t new_tweet_city_backup -c lib/backup.rb &> /tmp/new_tweet_city_cron_log
